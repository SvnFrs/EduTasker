    "prettier": "^3.6.2",
    "prisma": "^6.16.1",
    "tsx": "^4.20.5"
  },
  "peerDependencies": {
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.685.0",
    "@aws-sdk/s3-request-presigner": "^3.685.0",
    "@prisma/client": "^6.16.1",
    "@types/multer": "^2.0.0",
    "@types/xlsx": "^0.0.35",
    "bcryptjs": "^3.0.2",
